{
    "version": "0.2.0",
    "name": "jeremiemercier/php-library-glpi",
    "type": "library",
    "description": "GLPI API Client Library for PHP",
    "keywords": [
        "GLPI",
        "rest",
        "api",
        "client"
    ],
    "homepage": "https:\/\/glpi-project.github.io\/php-library-glpi\/",
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "Thierry Bugier",
            "email": "tbugier@teclib.com",
            "role": "Developer"
        },
        {
            "name": "Domingo Oropeza",
            "email": "doropeza@teclib.com",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "contact@teclib.com",
        "irc": "irc:\/\/irc.freenode.org\/glpi"
    },
    "require": {
        "php": ">=8.2",
        "guzzlehttp\/guzzle": "^7.4.5"
    },
    "autoload": {
        "psr-4": {
            "Glpi\\Api\\": "src\/Glpi\/Api\/"
        },
        "exclude-from-classmap": [
            "tests\/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Glpi\\Api\\Rest\\tests\\": "tests\/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
